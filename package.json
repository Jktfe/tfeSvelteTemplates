{
	"name": "tfe-svelte-templates",
	"version": "0.0.1",
	"description": "Production-ready Svelte 5 component templates with extensive documentation and examples",
	"private": true,
	"author": "James King <james@newmodel.vc>",
	"license": "MIT",
	"repository": {
		"type": "git",
